{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Neo",
  "colors": {
    "primary": "#888888",
    "light": "#E0E0E0",
    "dark": "#FFFFFF"
  },
  "favicon": "/nusa-black.ico",
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Home",
        "icon": "home",
        "groups": [
          {
            "group": "Welcome",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "About",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "dropdown": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Available Endpoint's",
            "pages": [
              "api-reference/chat/create-chat-completion-openai-compatible",
              "api-reference/embeddings/create-embeddings-openai-compatible",
              "api-reference/models/list-available-models",
              "api-reference/moderations/content-moderation-check",
              "api-reference/usage/get-usage-statistics",
              "api-reference/completions/openai-compatible-completions-text-only"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://x.com/NeosantaraAI",
          "icon": "twitter"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/neosa.svg",
    "dark": "/logo/neosa.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/NeosantaraAI",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "banner": {
    "content": "hello world [see](https://nusaai-edit.mintlify.com)"
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "errors": {
    "404": {
      "redirect": true
    }
  }
}